{
	"_version": "1.2.0",
	"sap.app": {
		"_version": "1.2.0",
		"id": "budget",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "http://localhost:3000/odata/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		},
		"sourceTemplate": {
			"id": "sap.ui.ui5-template-plugin.1worklist",
			"version": "1.36.2"
		}
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://task",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": false
		},
		"supportedThemes": ["sap_hcb", "sap_bluecrystal"],
		"fullWidth": true
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"rootView": {
			"viewName": "budget.view.App",
			"type": "XML",
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.36.0",
			"libs": {
				"sap.ui.core": {
					"minVersion": "1.36.0"
				},
				"sap.m": {
					"minVersion": "1.36.0"
				},
				"sap.ui.unified": {
					"minVersion": "1.36"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": false
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "budget.i18n.i18n"
				}
			},
			"": {
				"dataSource": "mainService",
				"settings": {
					"metadataUrlParams": {
						"sap-documentation": "heading"
					},
					"defaultBindingMode": "TwoWay",
					"defaultUpdateMethod": "PUT"
				},
				"preload": true
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "budget.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"bypassed": {
					"target": ["notFound"]
				},
				"async": true
			},
			"routes": [{
				"pattern": "",
				"name": "worklist",
				"target": ["worklist"]
			}, {
				"pattern": "Balance/{objectId}",
				"name": "object",
				"target": ["object"]
			}, {
				"name": "detAffectation",
				"pattern": "DetCC",
				"titleTarget": "",
				"greedy": false,
				"target": ["worklist", "detAffectation"]
			}, {
				"name": "upload",
				"pattern": "{month}/{year}/{id}/{description}",
				"titleTarget": "",
				"greedy": false,
				"target": ["upload"]
			}, {
				"name": "charts",
				"pattern": "Cchart",
				"titleTarget": "",
				"greedy": false,
				"target": ["charts"]
			}],
			"targets": {
				"worklist": {
					"viewName": "Worklist",
					"viewId": "worklist",
					"viewLevel": 1
				},
				"object": {
					"viewName": "Object",
					"viewId": "object",
					"viewLevel": 2
				},
				"objectNotFound": {
					"viewName": "ObjectNotFound",
					"viewId": "objectNotFound"
				},
				"notFound": {
					"viewName": "NotFound",
					"viewId": "notFound"
				},
				"upload": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Upload"
				},
				"detAffectation": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "CcDetermination",
					"viewLevel": 2,
					"viewId": "detAffectation"
				},
				"charts": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Charts",
					"viewLevel": 1
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css",
				"id": "style"
			}]
		}
	}
}